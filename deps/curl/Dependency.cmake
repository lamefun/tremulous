acquire_dependency( curl REQUIRED
  PKG_CONFIG libcurl
  FIND_PACKAGE CURL
  FIND_PACKAGE_INCLUDE_DIRS_VARS CURL_INCLUDE_DIRS
  FIND_PACKAGE_LIBRARIES_VARS CURL_LIBRARIES
  INTERNAL_WIN32_INCLUDE_DIRS
    "${CMAKE_CURRENT_LIST_DIR}/prebuilt/include"
  INTERNAL_WIN32_LIBRARIES
    "${CMAKE_CURRENT_LIST_DIR}/prebuilt/win32/libcurl.a"
  INTERNAL_WIN64_INCLUDE_DIRS
    "${CMAKE_CURRENT_LIST_DIR}/prebuilt/include"
  INTERNAL_WIN64_LIBRARIES
    "${CMAKE_CURRENT_LIST_DIR}/prebuilt/win64/libcurl.a" )
